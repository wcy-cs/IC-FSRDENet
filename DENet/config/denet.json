{
    "name": "fsr",
    "phase": "train", 
    "gpu_ids": [
        1
    ],
    "path": { 
        "log": "logs",
        "tb_logger": "tb_logger",
        "results": "results",
        "checkpoint": "checkpoint",
        "resume_state": null
    },
    "datasets": {
        "train": {
            "name": "x16",
            "mode": "LRHR",
            "dataroot": " ",
            "datatype": "img", 
            "l_resolution": 16, 
            "r_resolution": 256, 
            "batch_size": 4,
            "num_workers": 8,
            "use_shuffle": true,
            "data_len": -1 // -1 
        },
        "val": {
            "name": "x16",
            "mode": "LRHR",
            "dataroot": "t",
            "datatype": "img", 
            "l_resolution": 32,
            "r_resolution": 256,
            "data_len": -1 
        }
    },
    "model": {
        "which_model_G": "sr3", 
        "finetune_norm": false,
        "unet": {
            "in_channel": 6,
            "out_channel": 3,
            "inner_channel": 64,
            "channel_multiplier": [
                1,
                2,
                2,
                2,
                2
            ],
            "attn_res": [
                16
            ],
            "res_blocks": 2,
            "dropout": 0.2
        },
        "diffusion": {
            "image_size": 256,
            "channels": 3, 
            "conditional": true 
        }
    },
    "train": {
        "n_iter": 1000000,
        "val_freq": 1e4,
        "save_checkpoint_freq": 1e4,
        "print_freq": 200,
        "optimizer": {
            "type": "adam",
            "lr": 1e-4
        }
    },
}
